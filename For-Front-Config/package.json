{
  "name": "for_front_config",
  "author": "Anton Golovin",
  "license": "ISC",
  "version": "2.1.0",
  "description": "**For Front Config** позволяет легко и быстро начать работу над проектом с использованием \"Gulp + Webpack + Jest + Lint(s)\".",
  "repository": {
    "type": "git",
    "url": "git+https://git.forforce.com/forforce-dev/init-projects.git"
  },
  "homepage": "https://git.forforce.com/forforce-dev/init-projects/tree/master/For-Front-Config",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "npm run server",
    "server": "webpack-dev-server --config webpack.config.dev.js",
    "gulp": "gulp",
    "gulp:w": "gulp watch",
    "gulp:b": "gulp",
    "webpack": "webpack --config webpack.config.dev.js",
    "webpack:w": "webpack --config webpack.config.dev.js --watch --progress",
    "webpack:b": "webpack --config webpack.config.prod.js",
    "lint:js": "jest --projects \"./jest/jest.eslint.config.json\"",
    "lint:scss": "jest --projects \"./jest/jest.stylelint.config.json\"",
    "fix:js": "prettier-eslint --write \"./private/**/*.js\"",
    "fix:scss": "prettier-stylelint --write \"./private/**/*.scss\"",
    "test": "npm run lint:js && npm run lint:scss"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cache-loader": "^1.2.0",
    "css-loader": "^0.28.11",
    "dart-sass": "^1.13.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.1",
    "gulp-filter": "^5.0.0",
    "gulp-flatten": "^0.3.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svg-sprites": "^4.1.1",
    "gulp-tinypng-compress": "^1.2.1",
    "gulp-twig": "^1.2.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "imagemin-pngquant": "^5.0.0",
    "jest": "^23.5.0",
    "jest-runner-eslint": "^0.6.0",
    "jest-runner-stylelint": "^1.0.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.4",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.5.0",
    "twig-loader": "^0.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.2.1",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.12.6",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.6.0",
